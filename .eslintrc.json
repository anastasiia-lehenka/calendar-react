{
    "env": {
        "browser": true
    },
    "extends": ["airbnb", "airbnb/hooks"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-first-prop-new-line": 0,
        "max-len": [2, { "code": 120 }],
        "react/no-unescaped-entities": 0,
        "react/prop-types": 0
    }
}
